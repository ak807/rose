if ((N >= 4) && (T >= 1)) {
  for (c1=0;c1<=T-1;c1++) {
    for (c3=2*c1+2;c3<=2*c1+N-2;c3++) {
      S0(c1,2,-2*c1+c3);
    }
    for (c2=2*c1+3;c2<=2*c1+N-2;c2++) {
      S0(c1,-2*c1+c2,2);
      for (c3=2*c1+3;c3<=2*c1+N-2;c3++) {
        S1(c1,-2*c1+c2-1,-2*c1+c3-1);
        S0(c1,-2*c1+c2,-2*c1+c3);
      }
      S1(c1,-2*c1+c2-1,N-2);
    }
    for (c3=2*c1+3;c3<=2*c1+N-1;c3++) {
      S1(c1,N-2,-2*c1+c3-1);
    }
  }
}
